<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="284.0" prefWidth="347.0" spacing="20.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hrmanagement.dashboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button onAction="#adminclick" mnemonicParsing="false" text="Admin"/>
    <Button onAction="#employeeclick" mnemonicParsing="false" text="Employee"/>
    <Button onAction="#exitclick" mnemonicParsing="false" text="Exit"/>
    <Button onAction="#logoutclick" mnemonicParsing="false" text="Logout"/>
</VBox>
