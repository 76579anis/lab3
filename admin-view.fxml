<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hrmanagement.adminController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>


    <AnchorPane prefHeight="385.0" prefWidth="611.0">
      <children>
         <TableView layoutX="19.0" layoutY="7.0" prefHeight="303.0" prefWidth="401.0">
           <columns>
             <TableColumn prefWidth="75.0" fx:id="id" text="id" />
             <TableColumn prefWidth="75.0" fx:id="usename" text="username"/>
               <TableColumn prefWidth="75.0" fx:id="password" text="password" />
           </columns>
         </TableView>
         <Button onAction="#insert" layoutX="494.0" layoutY="38.0" mnemonicParsing="false" text="insert" />
         <Button onAction="#update" layoutX="494.0" layoutY="82.0" mnemonicParsing="false" text="update" />
         <Button onAction="#delete" layoutX="494.0" layoutY="125.0" mnemonicParsing="false" text="Button" />
         <Button onAction="#view" layoutX="494.0" layoutY="168.0" mnemonicParsing="false" text="Button" />
         <TextField layoutX="446.0" layoutY="208.0" />
         <TextField layoutX="447.0" layoutY="256.0" />
         <TextField layoutX="442.0" layoutY="298.0" />
      </children>
    </AnchorPane>
</VBox>
